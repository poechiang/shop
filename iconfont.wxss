@charset 'utf-8';

@font-face {
  font-family: 'iconfont';  /* project id 936976 */
  src: url('//at.alicdn.com/t/font_936976_z9o2y9u39pa.eot');
  src: url('//at.alicdn.com/t/font_936976_z9o2y9u39pa.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_936976_z9o2y9u39pa.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_936976_z9o2y9u39pa.woff') format('woff'),
