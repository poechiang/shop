  url('//at.alicdn.com/t/font_936976_z9o2y9u39pa.svg#iconfont') format('svg');
}

.ifont:before,.ifont:after,.iconfont{
    font-family:"iconfont" !important;
    font-size:1.33em;
    font-style:normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    vertical-align: .133em;
}
.ifont.large:before,.ifont.large:after,.iconfont.large:after{
    font-size:2em;
    display: block;
}

.ifont.medium:before,.ifont.medium:after,.iconfont.medium:after{
    font-size:1.67em;
    display: block;
}

[class *= 'icon-']:before,
[class *= -icon]:after{
    font-family:"iconfont" !important;
    font-size:1.33em;
    font-style:normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    vertical-align: .133em;
    margin:0 .05rem;
}

[class *= 'icon-'].small:before,[class *= '-icon'].mini:after{
    font-size:.83em;
    display: inline;
	vertical-align: .083em;
}

[class *= 'icon-'].small:before,[class *= '-icon'].small:after{
    font-size:1em;
    display: inline;
	vertical-align: .1em;
}
[class *= 'icon-'].large:before,[class *= '-icon'].large:after{
    font-size:2em;
    display: block;
}

[class *= 'icon-'].medium:before,[class *= '-icon'].medium:after{
    font-size:1.67em;
    display: block;
}

