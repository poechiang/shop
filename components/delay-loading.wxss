/* components/delay-loading.wxss */

.delay{
	text-align: center;
	color:#888888;
	font-size: 14px;
	position: relative
}
.delay::before{
	height:20px;
	width:20px;
	display: inline-block;
	vertical-align: middle;
	content: "";
	margin-right: 5px;
	background-image: url(https://res.shibu365.com/i/2019-02-21/97e3f95de64f4e51aa982da84b984141.gif);
	background-size: contain;
}

.delay.empty::before,
.delay.ended::before{
	display: none;
}